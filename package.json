{
  "name": "stylelint-plugin-arx",
  "version": "1.0.1",
  "description": "Stylelint plugin for managing imports",
  "main": "dist/index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "scripts": {
    "build": "babel src --out-dir dist  --extensions \".ts,.js\"",
    "lint:scss": "stylelint **/test/**/*.scss"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Arxivar/stylelint-plugin-arx.git"
  },
  "author": "Abletech",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.21",
    "minimatch": "^5.1.0",
    "source-map-loader": "^5.0.0",
    "ts-loader": "^9.5.1",
    "stylelint-plugin-arx": "file:."
  },
  "devDependencies": {
    "@babel/cli": "^7.25.7",
    "@babel/core": "^7.25.7",
    "@babel/plugin-transform-modules-commonjs": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@types/node": "^22.7.5",
    "@types/stylelint": "^14.0.0",
    "postcss": "^8.4.17",
    "postcss-scss": "^4.0.5",
    "prettier": "2.7.1",
    "stylelint": "14.13.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "typescript": "^5.6.2"
  }
}
